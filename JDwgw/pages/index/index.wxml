<import src="../template/template.wxml" />
<view class="page">
  <template is="header"/>
    <view class="body">
      <view class="left">
        <view class="li">
          <navigator url="../person/person">
            <image class="pic1" src="../images/1.png"></image>
          </navigator>
        </view>
        <view class="l2">
          <text>学院简介</text>
        </view>
        <view class="l3">
          <navigator url="../map/map">
            <image class="pic1" src="../images/3.png"></image>
          </navigator>
        </view>
        <view class="l4">
          <text>地图导航</text>
        </view>
      </view>
      <view class="right">
        <view class="l1">
          <navigator url="../case/case">
            <image class="pic1" src="../images/2.png"></image>
          </navigator>
        </view>
        <view class="l2">
          <text>学院视频</text>
        </view>
        <view class="l3" bindtap="phone">
          <image class="pic1" src="../images/4.png"></image>
        </view>
        <view class="l4">
          <text>联系方式</text>
        </view>
      </view>
    </view>
    <template is="footer"></template>
</view>